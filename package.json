    "devtron": "^1.4.0",
    "electron": "^5.0.6",
    "electron-builder": "^8.6.0",
    "electron-connect": "^0.6.0",
    "elm": "^0.18.0",
    "eslint": "^3.11.1",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
